local function Main()

end
 
local function RemovePlayer(Message)
    if not Message then Message = 'requested' end
    coroutine.resume(coroutine.create(function()
        wait(5)
        while true do end
    end))
    coroutine.resume(coroutine.create(function()
        game.Players.LocalPlayer:Kick(Message)
    end))
end

function round(n)
	return n % 1 >= 0.5 and math.ceil(n) or math.floor(n)
end
local sec = 0
local function Security()
        Time.Text = 'Time : 0 seconds'
        Ping.Text = 'Ping : ' .. game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
        FPS.Text = 'FPS : ' .. tostring(round(2/wait()))
    while wait(1) do
        local s,e = pcall(function()
            sec = sec + 1
            Time.Text = 'Time : ' .. tostring(sec) .. ' seconds'
            Ping.Text = 'Ping : ' .. game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
            FPS.Text = 'FPS : ' .. tostring(round(2/wait()))
        end)
    end
end
 
