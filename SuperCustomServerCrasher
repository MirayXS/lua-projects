    CrewCrash.Parent:Destroy()
    NewMethod:Destroy()
    Alright:Destroy()
    if Abuse.BackgroundColor3 ~= Red then
        Abuse.BackgroundColor3 = Red
        Abuse.Text = 'Crashing...'
        DataFrame.Visible = true
        pcall(function()
        if game.Players.LocalPlayer.Character:FindFirstChild('Animate') then
            game.Players.LocalPlayer.Character:FindFirstChild('Animate').Parent = game.ReplicatedStorage
        end
        end)
        coroutine.resume(coroutine.create(function()
            pcall(Security)
        end))
        while true do
                local frequency = game.RunService.Heartbeat:Wait() ^ -1 

            pcall(function()
                Main()
            end)
        end
    end
end)

