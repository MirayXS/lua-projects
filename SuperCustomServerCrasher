local Used = false
CrewCrash.MouseButton1Click:Connect(function()
    if Player.DataFolder.Information:FindFirstChild('Crew') and tonumber(Player.DataFolder.Information.Crew.Value) then
        made_by_Lerk7643:Destroy()
        if Used then
            return;
        end
        Used = true
        CrewCrash.Text = 'Crashing..'

        local function CrewCrasher()    
            local Crew = tonumber(Player.DataFolder.Information.Crew.Value)

                local TotalTools = 0
                local ToolCheck = 0
                repeat 
                    for i,v in pairs(Player.Backpack:GetChildren()) do
                        if v:IsA('Tool') and v.Name == '[SprayCan]' then
                            TotalTools = TotalTools + 1
                            v.Parent = Player.Character
                            pcall(function()
                                local bv = Instance.new("BodyVelocity", v.Handle)
                                bv.velocity = Vector3.new(0,0.1,0)
                                bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
                            end)
                        end
                    end
                    UNIT_5.Text = tostring(TotalTools) .. ' out of 1200'
                    game:GetService("ReplicatedStorage").MainEvent:FireServer("JoinCrew",tostring(Crew))
                    task.wait(1)
                    game:GetService("ReplicatedStorage").MainEvent:FireServer("LeaveCrew")
                                        task.wait(1)

                until TotalTools >= 1200
                
                Player.Character.LeftHand:Destroy()
                wait(3)
                coroutine.resume(coroutine.create(function()
                    game.Lighting.FogEnd = 1254124124
                    Player.Character.HumanoidRootPart.CFrame = CFrame.new(Player.Character.HumanoidRootPart.Position + Vector3.new(9999999,9999999,0))
                    local torso = game.Players.LocalPlayer.Character.Head
                    local bv = Instance.new("BodyVelocity", torso)
                    bv.velocity = Vector3.new(0,0.1,0)
                    bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
                end))
            
                for i,v in pairs(Player.Backpack:GetChildren()) do
                    if v:IsA('Tool') and v.Name == '[SprayCan]' then
                        v.Parent = Player.Character
                    end
                end
            
                local T = 0
                repeat
                    for i,v in pairs(Player.Backpack:GetChildren()) do
                        if v:IsA('Tool') and v.Name == '[SprayCan]' then
                            v.Parent = Player.Character
                        end
                    end
                    T = T + 1
                until T >= 35
             
                wait(3)
                DeleteChar()
            end
            CrewCrasher()
            Player.CharacterAdded:Connect(CrewCrasher)
    else
        announce('You have to join a crew', 'join crews on the bottom left', 5)
    end
end)
