pcall(function()
       game.StarterGui:SetCore("SendNotification", {
        Title = 'Every crash is currently patched';
        Text = 'Working on a fix';
        Duration = 20;
       })
  if not game.Players.LocalPlayer:IsInGroup(12052955) then
    coroutine.resume(coroutine.create(function()
       wait(10)
       game.StarterGui:SetCore("SendNotification", {
        Title = 'you are NOT in the group';
        Text = 'the crash will still work but our group link was copied to your clipboard thx';
        Duration = 20;
       })
       setclipboard('https://www.roblox.com/groups/12052955/Swag-mod#!/about')
    end))
  end
end)

