    local GLib = {}
    function GLib:NewTab(Name)
        local TabButton = Instance.new("TextButton")
        local SFrame = Instance.new("ScrollingFrame")
        local SFrameLayout = Instance.new("UIListLayout")
        
        TabButton.Name = Name or 'broken'
        TabButton.Text = Name or 'broken'
        TabButton.Parent = TabFrame
        TabButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        TabButton.BorderSizePixel = 0
        TabButton.Position = UDim2.new(0, 0, 1.16479306e-07, 0)
        TabButton.Size = UDim2.new(0, 120, 0, 31)
        TabButton.AutoButtonColor = false
        TabButton.Font = Enum.Font.GothamBlack
        TabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TabButton.TextSize = 14.000
        
        SFrame.Name = Name .. "SFrame" or 'blankSFrame'
        SFrame.Parent = ButtonsFrame
        SFrame.Active = true
        SFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        SFrame.BackgroundTransparency = 1.000
        SFrame.BorderSizePixel = 0
        SFrame.Position = UDim2.new(0.0379746817, 0, 0.0401567183, 0)
        SFrame.Size = UDim2.new(0, 297, 0, 312)
        SFrame.ScrollBarThickness = 7
        
        SFrameLayout.Name = "SFrameLayout"
        SFrameLayout.Parent = SFrame
        SFrameLayout.SortOrder = Enum.SortOrder.LayoutOrder
        SFrameLayout.Padding = UDim.new(0, 6)
    end
    function GLib:NewSearch(Tab)
        local Searchbar = Instance.new("TextBox")
                
