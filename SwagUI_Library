local Library = {}
function Library:CreateMain()
    local ScreenGui = Instance.new("ScreenGui")
    local TabsCreated = 0
    ScreenGui.Name = 'Swag mode'
    local MainFrame = Instance.new("Frame")
    local Frame = Instance.new("Frame")
    local TabsFrame = Instance.new("Frame")
    local Title = Instance.new("TextLabel")
    local UITextSizeConstraint_4 = Instance.new("UITextSizeConstraint")
    local UIListLayout_3 = Instance.new("UIListLayout")
    local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
    local DiscordButton = Instance.new("TextButton")
    local UITextSizeConstraint_6 = Instance.new("UITextSizeConstraint")
    local Line = Instance.new("TextLabel")
    local UITextSizeConstraint_7 = Instance.new("UITextSizeConstraint")
    local TargetTextbox = Instance.new("TextBox")
    local UITextSizeConstraint_8 = Instance.new("UITextSizeConstraint")

    ScreenGui.Parent = game:GetService('CoreGui')
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    
    MainFrame.Name = "MainFrame"
    MainFrame.Parent = ScreenGui
    MainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    MainFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
    MainFrame.BorderSizePixel = 0
    MainFrame.Position = UDim2.new(0.27957058, 0, 0.499272168, 0)
    MainFrame.Size = UDim2.new(0.336005837, 0, 0.425036341, 0)
    MainFrame.Active = true
    MainFrame.Draggable = true
    
    TabsFrame.Name = "TabsFrame"
    TabsFrame.Parent = MainFrame
    TabsFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    TabsFrame.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
    TabsFrame.BorderSizePixel = 0
    TabsFrame.Position = UDim2.new(0.182212576, 0, 0.568493187, 0)
    TabsFrame.Size = UDim2.new(0.2885032, 0, 0.772260308, 0)

    DiscordButton.Name = "DiscordButton"
    DiscordButton.Parent = MainFrame
    DiscordButton.AnchorPoint = Vector2.new(0.5, 0.5)
    DiscordButton.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
    DiscordButton.BorderSizePixel = 0
    DiscordButton.Position = UDim2.new(0.930585682, 0, 0.0924657658, 0)
    DiscordButton.Size = UDim2.new(0.0715835169, 0, 0.113013707, 0)
    DiscordButton.Font = Enum.Font.SourceSans
    DiscordButton.Text = "D"
    DiscordButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    DiscordButton.TextScaled = true
    DiscordButton.TextSize = 14.000
    DiscordButton.TextWrapped = true
    
    UITextSizeConstraint_6.Parent = DiscordButton
    UITextSizeConstraint_6.MaxTextSize = 33
    
    if TARGET_BAR_ENABLED then
        Line.Name = "Line"
        Line.Parent = MainFrame
        Line.AnchorPoint = Vector2.new(0.5, 0.5)
        Line.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Line.BackgroundTransparency = 1.000
        Line.BorderSizePixel = 0
        Line.Position = UDim2.new(0.581308961, 0, 0.109589055, 0)
        Line.Size = UDim2.new(0.559724867, 0, 0.109589055, 0)
        Line.Font = Enum.Font.SourceSans
        Line.Text = "_______________________________"
        Line.TextColor3 = Color3.fromRGB(0, 0, 0)
        Line.TextScaled = true
        Line.TextSize = 20.000
        Line.TextWrapped = true
        
        UITextSizeConstraint_7.Parent = Line
        UITextSizeConstraint_7.MaxTextSize = 20
        
        TargetTextbox.Name = "TargetTextbox"
        TargetTextbox.Parent = MainFrame
        TargetTextbox.AnchorPoint = Vector2.new(0.5, 0.5)
        TargetTextbox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TargetTextbox.BackgroundTransparency = 1.000
        TargetTextbox.BorderSizePixel = 0
        TargetTextbox.Position = UDim2.new(0.578562737, 0, 0.071917817, 0)
        TargetTextbox.Size = UDim2.new(0.430645943, 0, 0.106164396, 0)
        TargetTextbox.Font = Enum.Font.SourceSans
        TargetTextbox.PlaceholderColor3 = Color3.fromRGB(0, 0, 0)
        TargetTextbox.PlaceholderText = "Target's Username"
        TargetTextbox.Text = ""
        TargetTextbox.TextColor3 = Color3.fromRGB(0, 0, 0)
        TargetTextbox.TextScaled = true
        TargetTextbox.TextSize = 14.000
        TargetTextbox.TextWrapped = true
        
        UITextSizeConstraint_8.Parent = TargetTextbox
        UITextSizeConstraint_8.MaxTextSize = 28
        function ShrinkName()
            TargetTextbox.FocusLost:connect(function()
                for i,v in pairs(game.Players:GetChildren()) do
                    if (string.sub(string.lower(v.Name),1,string.len(TargetTextbox.Text))) == string.lower(TargetTextbox.Text) then
                        TargetTextbox.Text = v.Name
                    end
                end
            end)
        end
        ShrinkName()
        function GetTarget()
        local Enemy = game.Players:FindFirstChild(TargetTextbox.Text)
            if Enemy then
                return Enemy
            end
        end 
    end

    Title.Name = "Title"
    Title.Parent = MainFrame
    Title.AnchorPoint = Vector2.new(0.5, 0.5)
    Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Title.BackgroundTransparency = 1.000
    Title.BorderSizePixel = 0
    Title.Position = UDim2.new(0.236442521, 0, 0.0924657658, 0)
    Title.Size = UDim2.new(0.425162613, 0, 0.143835634, 0)
    Title.Font = Enum.Font.Nunito
    Title.Text = "Swag mode"
    Title.TextColor3 = Color3.fromRGB(0, 0, 0)
    Title.TextScaled = true
    Title.TextSize = 14.000
    Title.TextWrapped = true
    Title.TextXAlignment = Enum.TextXAlignment.Left
    
    UITextSizeConstraint_4.Parent = Title
    UITextSizeConstraint_4.MaxTextSize = 48
    
    UIListLayout_3.Parent = TabsFrame
    UIListLayout_3.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout_3.Padding = UDim.new(0, 9)
    
    UIAspectRatioConstraint.Parent = MainFrame
    UIAspectRatioConstraint.AspectRatio = 1.579

    local GamerLibrary = {}
    function GamerLibrary:NewTab(Name)
        local s,e = pcall(function()
        local TabButton = Instance.new("TextButton")
        UITextSizeConstraint_5 = Instance.new("UITextSizeConstraint")
        TabButton.Name = Name or 'Blank_Button'
        TabButton.Parent = TabsFrame
        TabButton.AnchorPoint = Vector2.new(0.5, 0.5)
        TabButton.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
        TabButton.BorderSizePixel = 0
        TabButton.Position = UDim2.new(0.496240705, 0, 0.128603116, 0)
        TabButton.Size = UDim2.new(1, 0, 0.133037671, 0)
        TabButton.Font = Enum.Font.SourceSans
        TabButton.Text = Name or 'Blank_Button'
        TabButton.TextColor3 = Color3.fromRGB(0, 0, 0)
        TabButton.TextScaled = true
        TabButton.TextSize = 14.000
        TabButton.TextWrapped = true
                
        local TabSFrame = Instance.new("ScrollingFrame")
        local UIListLayout_2 = Instance.new("UIListLayout")
        local UIListLayout_22 = Instance.new("UIListLayout")
       
        TabSFrame.Name = Name .. 'SFrame' or 'BlankSFrame'
        TabSFrame.Parent = MainFrame
        TabSFrame.Active = true
        TabSFrame.AnchorPoint = Vector2.new(0.5, 0.5)
        TabSFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        TabSFrame.BackgroundTransparency = 0.800
        TabSFrame.BorderSizePixel = 0
        TabSFrame.Position = UDim2.new(0.678958774, 0, 0.568493187, 0)
        TabSFrame.Size = UDim2.new(0.605205953, 0, 0.773972511, 0)
        
        local Frame = Instance.new('Frame')
        Frame.Parent = TabSFrame
        Frame.AnchorPoint = Vector2.new(0.5, 0.5)
        Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Frame.BackgroundTransparency = 1.000
        Frame.BorderSizePixel = 0
        Frame.Position = UDim2.new(0.498207986, 0, 1.00442493, 0)
        Frame.Size = UDim2.new(1, 0, 5.2102232, 0)
        
        TabButton.MouseButton1Click:Connect(function()
            local s,e = pcall(function()
            for i,v in pairs(MainFrame:GetChildren()) do
                if string.find(v.Name, 'SFrame') then
                    if v.Name ~= TabSFrame.Name then
                        v.Visible = false
                    else
                        v.Visible = true
                    end
                end
            end
            end)
        end)
        
        UIListLayout_2.Parent = Frame
        UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout_2.Padding = UDim.new(0, 5)
    
        UITextSizeConstraint_5.Parent = TabButton
        UITextSizeConstraint_5.MaxTextSize = 29
        UIListLayout_22.Parent = TabSFrame
        UIListLayout_22.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout_22.Padding = UDim.new(0,5)
        if TabsCreated == 0 then
            TabsCreated = 1
        else
            TabSFrame.Visible = false
        end

        end)
        if e then print(e) end
    end
    
    function GamerLibrary:NewButton(Name, Tab, Action)
        local Button = Instance.new("TextButton")
        local UITextSizeConstraint = Instance.new("UITextSizeConstraint")
        Button.Name = Name
        Button.Parent = game:GetService("CoreGui")["Swag mode"].MainFrame:FindFirstChild(Tab..'SFrame').Frame
        Button.AnchorPoint = Vector2.new(0.5, 0.5)
        Button.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
        Button.BorderSizePixel = 0
        Button.Text = Name
        Button.Position = UDim2.new(0.478494614, 0, 0.0057232054, 0)
        Button.Size = UDim2.new(0.956989229, 0, 0.0114464052, 0)
        Button.Font = Enum.Font.Nunito
        Button.TextColor3 = Color3.fromRGB(0, 0, 0)
        Button.TextScaled = true
        Button.TextSize = 14.000
        Button.Active = true
        Button.Visible = true
        Button.TextWrapped = true
        Button.ZIndex = 444
        
        UITextSizeConstraint.Parent = Button
        UITextSizeConstraint.MaxTextSize = 82
        Button.MouseButton1Click:Connect(Action)
    end
    function GamerLibrary:NewToggle(Name, Tab, Action)
        local Toggle = Instance.new("TextButton")
        local ToggleColor = Instance.new("TextButton")
        local UITextSizeConstraint_2 = Instance.new("UITextSizeConstraint")
        local UITextSizeConstraint_3 = Instance.new("UITextSizeConstraint")
        
        Toggle.Name = Name
        Toggle.Parent = game:GetService("CoreGui")["Swag mode"].MainFrame:FindFirstChild(Tab..'SFrame').Frame
        Toggle.AnchorPoint = Vector2.new(0.5, 0.5)
        Toggle.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
        Toggle.BorderSizePixel = 0
        Toggle.Position = UDim2.new(0.478494614, 0, 0.028345881, 0)
        Toggle.Size = UDim2.new(0.956989229, 0, 0.0111740315, 0)
        Toggle.Font = Enum.Font.Nunito
        Toggle.Text = Name
        Toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
        Toggle.TextScaled = true
        Toggle.TextSize = 14.000
        Toggle.TextWrapped = true
        
        ToggleColor.Name = "ToggleColor"
        ToggleColor.Parent = Toggle
        ToggleColor.AnchorPoint = Vector2.new(0.5, 0.5)
        ToggleColor.BackgroundColor3 = Color3.new(255,0,0)
        ToggleColor.BorderSizePixel = 0
        ToggleColor.Position = UDim2.new(0.958801746, 0, 0.487610728, 0)
        ToggleColor.Size = UDim2.new(0.078651689, 0, 0.999999881, 0)
        ToggleColor.Font = Enum.Font.SourceSans
        ToggleColor.Text = ""
        ToggleColor.TextColor3 = Color3.fromRGB(0, 0, 0)
        ToggleColor.TextScaled = true
        ToggleColor.TextSize = 14.000
        ToggleColor.TextWrapped = true
        
        UITextSizeConstraint_2.Parent = ToggleColor
        UITextSizeConstraint_2.MaxTextSize = 82
        
        UITextSizeConstraint_3.Parent = Toggle
        UITextSizeConstraint_3.MaxTextSize = 82
        local RED = Color3.new(255,0,0)
        local GREEN = Color3.new(0,255,0)
        ToggleColor.BackgroundColor3 = RED

        Toggle.MouseButton1Click:Connect(function()
            if ToggleColor.BackgroundColor3 == RED then
                ToggleColor.BackgroundColor3 = GREEN
            else
                ToggleColor.BackgroundColor3 = RED
            end
        end)
        Toggle.MouseButton1Click:Connect(function()
            repeat
                Action()
                game.RunService.RenderStepped:Wait()
            until ToggleColor.BackgroundColor3 == RED
        end)
    end
    return GamerLibrary;
end
return Library

